<div class="row">
  <div class="col-sm-4">
    <%= form_for [@community, @post] do |form| %>
    <div class="form-group">
      <%= label_tag :title %>
      <%= form.text_field :title, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= label_tag :body %>
      <%= form.text_field :body, class: "form-control" %>
    </div>

    <%= form.submit 'Submit', class: "btn btn-primary mt-4" %>
    <% end %>
  </div>
</div>
